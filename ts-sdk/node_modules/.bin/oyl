#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/sdk/bin/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/sdk/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/sdk/bin/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/sdk/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules/@oyl/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/@oyl+sdk@1.17.0_@swc+core@1.15.3_@swc+wasm@1.15.3_@types+node@20.19.24_typescript@5.9.3/node_modules:/Users/gabe/subfrost-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@oyl/sdk/bin/oyl.js" "$@"
else
  exec node  "$basedir/../@oyl/sdk/bin/oyl.js" "$@"
fi
