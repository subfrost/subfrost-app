{
  "name": "subfrost-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed:invite-codes": "npx tsx prisma/seed-invite-codes.ts",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --exclude='**/*.integration.test.ts' --exclude='**/e2e/**' --exclude='**/__tests__/sdk/**' --exclude='.external-build/**' --exclude='**/useFrbtcPremium.test.ts' --exclude='**/vaultCalldata.test.ts' --exclude='**/utxoParsing.test.ts' --exclude='**/useSwapQuotes.wrap-unwrap.test.ts' --exclude='**/wasm-init.test.ts'",
    "test:utils": "vitest run utils/__tests__ lib/**/__tests__ app/**/__tests__",
    "test:hooks": "vitest run hooks/__tests__",
    "test:sdk": "vitest run __tests__/sdk",
    "test:integration": "INTEGRATION=true vitest run __tests__/sdk",
    "test:integration:no-puppeteer": "INTEGRATION=true SKIP_PUPPETEER=true vitest run __tests__/sdk",
    "test:integration:btc": "INTEGRATION=true vitest run __tests__/sdk/btc-send.integration.test.ts",
    "test:legacy": "npm run test:math && npm run test:calldata && npm run test:utxo",
    "test:math": "npx tsx hooks/__tests__/useFrbtcPremium.test.ts && npx tsx hooks/__tests__/useSwapQuotes.integration.test.ts",
    "test:calldata": "npx tsx hooks/__tests__/vaultCalldata.test.ts",
    "test:utxo": "npx tsx hooks/__tests__/utxoParsing.test.ts",
    "test:e2e:vault": "npx tsx e2e/vault-e2e.test.ts",
    "test:e2e:swap": "npx tsx e2e/swap-e2e.test.ts",
    "test:e2e:wallet": "npx tsx e2e/wallet-e2e.test.ts",
    "test:e2e": "npm run test:e2e:wallet && npm run test:e2e:swap && npm run test:e2e:vault"
  },
  "dependencies": {
    "@alkanes/ts-sdk": "https://pkg.alkanes.build/dist/@alkanes/ts-sdk?v=0.1.4-f93aaf9",
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@bprogress/next": "^3.2.12",
    "@jest/globals": "^30.2.0",
    "@noble/hashes": "^2.0.1",
    "@tanstack/react-query": "^5.83.0",
    "bignumber.js": "^9.3.1",
    "bitcoin-address-validation": "^3.0.0",
    "bitcoinjs-lib": "^7.0.0",
    "bitcoinjs-message": "^2.2.0",
    "ecpair": "^3.0.0",
    "jest": "^29.7.0",
    "lightweight-charts": "^5.1.0",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "qrcode": "^1.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "ioredis": "^5.6.1",
    "@prisma/client": "^6.6.0",
    "tiny-secp256k1": "^2.2.4",
    "use-debounce": "^10.0.6",
    "varuint-bitcoin": "^2.0.0"
  },
  "devDependencies": {
    "@noble/curves": "^2.0.1",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@scure/base": "^2.0.0",
    "@scure/btc-signer": "^2.0.1",
    "@tailwindcss/postcss": "^4",
    "@types/bip32": "^2.0.4",
    "@types/bip39": "^3.0.4",
    "@types/node": "^20",
    "@types/puppeteer": "^5.4.7",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.15",
    "bip32": "^5.0.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "global": "^4.4.0",
    "happy-dom": "^20.0.11",
    "process": "^0.11.10",
    "puppeteer": "^24.29.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.15",
    "prisma": "^6.6.0",
    "@types/ioredis": "^5.0.0"
  }
}
