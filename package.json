    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.15",
    "bip32": "^5.0.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "global": "^4.4.0",
    "happy-dom": "^20.0.11",
    "process": "^0.11.10",
    "puppeteer": "^24.29.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-plugin-top-level-await": "^1.6.0",
