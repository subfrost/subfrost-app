    "bitcoin-address-validation": "^3.0.0",
    "bitcoinjs-lib": "^7.0.0",
    "bitcoinjs-message": "^2.2.0",
    "ecpair": "^3.0.0",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "qrcode": "^1.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tiny-secp256k1": "^2.2.4",
    "use-debounce": "^10.0.6",
    "varuint-bitcoin": "^2.0.0"
  },
  "devDependencies": {
    "@noble/curves": "^2.0.1",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
